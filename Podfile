
use_frameworks!

target 'AnobiKit_Example_iOS' do
  platform :ios, '9.3'
  pod 'AnobiKit', :path => './'

  target 'AnobiKit_Tests_iOS' do
      inherit! :search_paths
  end
end

target 'AnobiKit_Example_macOS' do
  platform :macos, '10.12'
  pod 'AnobiKit', :path => './'
  
  target 'AnobiKit_Tests_macOS' do
    inherit! :search_paths
  end
end
